# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp


# Ignore all of our files and directories with .bak
*.bak

_Obs


# where our secrets are stored (generated by Figaro)
/config/application.yml

# Not storing a database file
# Create a local one with database.example.yml
/config/database.yml
# Ignore application configuration
/config/application.yml
